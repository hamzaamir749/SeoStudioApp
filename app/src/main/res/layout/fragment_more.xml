<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true"
    android:background="@color/white">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginBottom="@dimen/_3sdp"
        app:layout_constraintBottom_toTopOf="@+id/native_container_setting"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintTop_toBottomOf="@+id/proCardview">

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/premium_cardview"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:layout_marginHorizontal="@dimen/_8sdp"
                android:layout_marginTop="@dimen/_8sdp"
                app:cardBackgroundColor="@color/searchView"

                app:cardCornerRadius="@dimen/_10sdp"
                app:cardElevation="0dp"
                app:rippleColor="@android:color/transparent">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center_vertical"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/pro_icon"
                    android:textSize="@dimen/_13sdp"
                    app:tint="#666666" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginBottom="@dimen/_7sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="Premium version"
                    android:textColor="#666666"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="bold" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="Enjoy Ads free version"
                    android:textColor="@android:color/darker_gray"
                    android:textSize="@dimen/_10sdp" />

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/Shareapp_cardview"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:layout_marginHorizontal="@dimen/_8sdp"
                android:layout_marginTop="@dimen/_8sdp"
                app:cardBackgroundColor="@color/searchView"

                app:cardCornerRadius="@dimen/_10sdp"
                app:cardElevation="0dp"
                app:rippleColor="@android:color/transparent">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center_vertical"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/share_icon"
                    android:textSize="@dimen/_13sdp"
                    app:tint="#666666" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginBottom="@dimen/_7sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="Share App"
                    android:textColor="#666666"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="bold" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="Share this fantastic app with friends"
                    android:textColor="@android:color/darker_gray"
                    android:textSize="@dimen/_10sdp" />

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/rateus_cardview"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:layout_marginHorizontal="@dimen/_8sdp"
                android:layout_marginTop="@dimen/_8sdp"
                app:cardBackgroundColor="@color/searchView"

                app:cardCornerRadius="@dimen/_10sdp"
                app:cardElevation="0dp"
                app:rippleColor="@android:color/transparent">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center_vertical"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/rate_us_icon"
                    android:textSize="@dimen/_11sdp"
                    app:tint="#666666" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginBottom="@dimen/_7sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="Rate Us"
                    android:textColor="#666666"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="bold" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="Rate us how we work"
                    android:textColor="@android:color/darker_gray"
                    android:textSize="@dimen/_10sdp" />

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/feedback_cardview"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:layout_marginHorizontal="@dimen/_8sdp"
                android:layout_marginTop="@dimen/_8sdp"
                app:cardBackgroundColor="@color/searchView"

                app:cardCornerRadius="@dimen/_10sdp"
                app:cardElevation="0dp"
                app:rippleColor="@android:color/transparent">

                <ImageView
                    android:layout_width="@dimen/_20sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_gravity="start|center_vertical"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/feedback_icon"
                    android:textSize="@dimen/_11sdp"
                    app:tint="#666666" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginBottom="@dimen/_7sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="Feedback"
                    android:textColor="#666666"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="bold" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="Need help?"
                    android:textColor="@android:color/darker_gray"
                    android:textSize="@dimen/_10sdp" />

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/website_cardivew"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:layout_marginHorizontal="@dimen/_8sdp"
                android:layout_marginTop="@dimen/_8sdp"
                app:cardBackgroundColor="@color/searchView"

                app:cardCornerRadius="@dimen/_10sdp"
                app:cardElevation="0dp"
                app:rippleColor="@android:color/transparent">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center_vertical"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/website_icon"
                    android:textSize="@dimen/_11sdp"
                    app:tint="#666666" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginBottom="@dimen/_7sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="Website"
                    android:textColor="#666666"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="bold" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="See our website "
                    android:textColor="@android:color/darker_gray"
                    android:textSize="@dimen/_10sdp" />

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:id="@+id/youtube_cardivew"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:layout_marginHorizontal="@dimen/_8sdp"
                android:layout_marginTop="@dimen/_8sdp"
                app:cardBackgroundColor="@color/searchView"

                app:cardCornerRadius="@dimen/_10sdp"
                app:cardElevation="0dp"
                app:rippleColor="@android:color/transparent">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start|center_vertical"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/youtube_icon"
                    android:textSize="@dimen/_11sdp"
                    app:tint="#666666" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginBottom="@dimen/_7sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="Youtube channel"
                    android:textColor="#666666"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="bold" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="See our video about keyword plans "
                    android:textColor="@android:color/darker_gray"
                    android:textSize="@dimen/_10sdp" />

            </com.google.android.material.card.MaterialCardView>


            <com.google.android.material.card.MaterialCardView
                android:id="@+id/blogpost_cardivew"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_60sdp"
                android:layout_marginHorizontal="@dimen/_8sdp"
                android:layout_marginTop="@dimen/_8sdp"
                android:layout_marginBottom="@dimen/_10sdp"
                app:cardBackgroundColor="@color/searchView"

                app:cardCornerRadius="@dimen/_10sdp"
                app:cardElevation="0dp"
                app:rippleColor="@android:color/transparent">

                <ImageView
                    android:layout_width="@dimen/_20sdp"
                    android:layout_height="@dimen/_20sdp"
                    android:layout_gravity="start|center_vertical"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:src="@drawable/blog_icon"
                    android:textSize="@dimen/_11sdp"
                    app:tint="#666666" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginBottom="@dimen/_7sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="Blog post"
                    android:textColor="#666666"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="bold" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginStart="@dimen/_55sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:fontFamily="@font/lexend"
                    android:gravity="center_vertical"
                    android:text="See latest trending blogs about keywords"
                    android:textColor="@android:color/darker_gray"
                    android:textSize="@dimen/_10sdp" />

            </com.google.android.material.card.MaterialCardView>


        </LinearLayout>
    </ScrollView>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/native_container_setting"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginHorizontal="@dimen/_5sdp"
        android:layout_marginBottom="@dimen/_2sdp"
        android:orientation="vertical"
        android:padding="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">




        <FrameLayout
            android:id="@+id/admob_native_container_setting"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>